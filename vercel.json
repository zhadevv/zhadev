{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/page/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/page/xxx/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/src/page/(.*)",
      "dest": "/src/page/$1"
    },
    {
      "src": "/src/page/xxx/(.*)",
      "dest": "/src/page/xxx/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/src/js/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/src/css/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/src/page/(.*)\\.js",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/src/page/(.*)\\.css",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/src/page/xxx/(.*)\\.js",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/src/page/xxx/(.*)\\.css",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/src/(.*)\\.js",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/src/(.*)\\.css",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/js/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/css/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/env",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/config.yaml",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/robots.txt",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/\\.env",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/\\.git",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/\\.git/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/package\\.json",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/vercel\\.json",
      "destination": "/404.html",
      "statusCode": 404
    }
  ]
}